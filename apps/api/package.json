{
  "name": "api",
  "version": "0.0.1",
  "description": "AggreGram API",
  "private": true,
  "scripts": {
    "dev": "nest start --watch",
    "start": "node dist/main",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --fix",
